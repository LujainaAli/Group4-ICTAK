package com.ictwebsite.test;

import org.testng.annotations.Test;
import org.testng.AssertJUnit;
import org.testng.Assert;
import org.testng.annotations.Test;

import com.ictwebsite.constants.AutomationConstants;
import com.ictwebsite.pages.EventsPage;
import com.ictwebsite.pages.TechathlonPAGE;

public class EventsTestClass extends TestBase {

	EventsPage obj = null;
	TechathlonPAGE obj1 = null;

	@Test

	public void testcase1() {
		obj = new EventsPage(driver);
		obj.eventsFunc();
		obj.icsetPage();
		String actualICSETpg = driver.getCurrentUrl();
	
		AssertJUnit.assertEquals(actualICSETpg, AutomationConstants.ICSETloading);
		
	}

	@Test

	public void testcase2() {
		obj = new EventsPage(driver);
		obj.eventsFunc();
		obj.icsetPage();
		obj.agenda();
		String agendaActual = obj.agendaContent();
		AssertJUnit.assertEquals(agendaActual, AutomationConstants.agendaExpected);
	}

	@Test

	public void testcase3() {
		obj = new EventsPage(driver);
		obj.eventsFunc();
		obj.icsetPage();
		obj.applynow();
		obj.name();
		obj.email();
		obj.phonenumb();
		obj.register();
		AssertJUnit.assertEquals(obj.successmsg(),AutomationConstants.regSuccessMsg );
		
	}

	@Test

	public void testcase4() throws InterruptedException {
		obj = new EventsPage(driver);
		obj.eventsFunc();
		obj1 = new TechathlonPAGE(driver);

		obj1.techath();
		obj1.apply2();
		obj1.name2();
		obj1.email2();
		obj1.mobNum2();
		obj1.register2();
	AssertJUnit.assertEquals(obj.successmsg(), AutomationConstants.regSuccessMsg);	

	}

	@Test

	public void testcase5() throws InterruptedException {
		obj = new EventsPage(driver);
		obj.eventsFunc();
		obj1 = new TechathlonPAGE(driver);
		obj1.techath();
		obj1.apply2();
		obj1.name3();
		obj1.email3();
		obj1.mobNum3();
		obj1.register2();
		AssertJUnit.assertEquals(obj.successmsg(), AutomationConstants.regUnsuccess);
	}

	@Test

	public void testcase6() throws InterruptedException {
		obj = new EventsPage(driver);
		obj.eventsFunc();
		obj1 = new TechathlonPAGE(driver);
		obj1.techath();
		obj1.apply2();
		obj1.blankvalues();
		boolean regisStatus = obj1.registrationDisabled();
         AssertJUnit.assertFalse(regisStatus);  
		
	}

	@Test

	public void testcase7() {
		obj = new EventsPage(driver);
		obj.eventsFunc();
		obj.icsetPage();
		obj.applynow();
		obj.name4();
		obj.email4();
		obj.phonenumb4();
		obj.register();
		
	}

	@Test
	
	public void testcase8() {
		obj = new EventsPage(driver);
	obj.eventsFunc();
	obj.icsetPage();
	obj.membershipDetails();	
String joinMembershipform =	obj.join();
	AssertJUnit.assertEquals(joinMembershipform,AutomationConstants.getAcess );
	}
	

	@Test
	
	public void testcase9() throws InterruptedException {
		obj = new EventsPage(driver);
		obj.eventsFunc();
		obj1 = new TechathlonPAGE(driver);
		obj1.techath();
		obj1.objectives();
	boolean idenKey= 	obj1.identifyKeywords();
	AssertJUnit.assertTrue(idenKey);
	}
	
	
	@Test
	
	public void testcase10() throws InterruptedException {
		obj = new EventsPage(driver);
		obj.eventsFunc();
		obj.icsetPage();
		obj.training();
	String trainVal =	obj.trainingValue();
	AssertJUnit.assertEquals(trainVal, AutomationConstants.trainingval);	
		
	}
}
